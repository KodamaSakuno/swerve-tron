
// common start
.account-empty {
  white-space: nowrap;
  width: fit-content;
  margin-left: 10px;
  pointer-events: auto;
}
// common end

/deep/ .account-wallet-dialog {
  .adc-account {
    background-color: rgb(33, 36, 41);
    padding: 0rem 1rem;
  }
  .adcc-content {
    padding: 1rem;
    border: 1px solid rgb(64, 68, 79);
    border-radius: 20px;
    position: relative;
    display: grid;
    row-gap: 12px;
    margin-bottom: 20px;
  }
  .adccc-header {
    display: flex;
    flex-flow: row nowrap;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    font-weight: 400;
    color: rgb(255, 255, 255);
  }
  .adccch-title {
    display: flex;
    flex-flow: row nowrap;
    -webkit-box-align: center;
    align-items: center;
    width: initial;
    font-size: 0.825rem;
    font-weight: 500;
    color: rgb(108, 114, 132);
  }
  .adccch-change {
    text-align: center;
    outline: none;
    border: 1px solid transparent;
    text-decoration: none;
    display: flex;
    -webkit-box-pack: center;
    justify-content: center;
    flex-wrap: nowrap;
    -webkit-box-align: center;
    align-items: center;
    cursor: pointer;
    position: relative;
    z-index: 1;
    background-color: rgba(21, 61, 111, 0.44);
    color: rgb(109, 168, 255);
    border-radius: 8px;
    width: fit-content;
    margin-left: 8px;
    padding: 4px 6px;
    font-size: 0.825rem;
    font-weight: 400;
    line-height: inherit;
    box-sizing: border-box;
    margin: 0px;
    min-width: 0px;
    appearance: none;
    &:hover {
      cursor: pointer;
      background-color: rgba(55, 107, 173, 0.44);
    }
  }
  .account-address {
    display: flex;
    flex-flow: row nowrap;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    font-weight: 400;
    color: rgb(255, 255, 255);
  }
  .aa-content {
    display: flex;
    flex-flow: row nowrap;
    -webkit-box-align: center;
    align-items: center;
    -webkit-box-pack: justify;
    justify-content: space-between;
    min-width: 0px;
    width: 100%;
    font-weight: 500;
    font-size: 1.25rem;
    div {
      display: flex;
      flex-flow: row nowrap;
      -webkit-box-align: center;
      align-items: center;
    }
    p {
      min-width: 0px;
      margin: 0px;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
    }
    .aac-avatar {
      display: flex;
      flex-flow: row nowrap;
      -webkit-box-align: center;
      align-items: center;
      -webkit-box-pack: center;
      justify-content: center;
      margin-right: 8px;
    }
  }
  .adccc-footer {
    display: flex;
    flex-flow: row nowrap;
    -webkit-box-pack: justify;
    justify-content: space-between;
    -webkit-box-align: center;
    align-items: center;
    font-weight: 400;
    color: rgb(255, 255, 255);
    div {
      display: flex;
      flex-flow: row nowrap;
      -webkit-box-align: center;
      align-items: center;
    }
  }
  .af-content {
    -webkit-box-pack: justify;
    justify-content: space-between;
    min-width: 0px;
    width: 100%;
    font-weight: 500;
    font-size: 1.25rem;
  }
  .afc-button {
    border: none;
    background: none;
    cursor: pointer;
    font-weight: 500;
    color: rgb(108, 114, 132);
    flex-shrink: 0;
    display: flex;
    text-decoration: none;
    font-size: 0.825rem;
    &:hover {
      text-decoration: none;
      color: rgb(195, 197, 203);
    }
    &:focus {
      outline: none;
    }
  }
  .afcb-icon {
    margin-left: 0.25rem;
    font-size: 0.825rem;
    display: flex;
    flex-flow: row nowrap;
    -webkit-box-align: center;
    align-items: center;
    width: 16px;
    height: 16px;
  }
  .afcb-link {
    color: rgb(108, 114, 132);
    margin-left: 1rem;
    font-size: 0.825rem;
    display: flex;
    text-decoration: none;
    cursor: pointer;
    font-weight: 500;
    &:hover {
      color: rgb(195, 197, 203);
      text-decoration: underline;
    }
  }

  .adc-transaction {
    flex-flow: column nowrap;
    padding: 1.5rem;
    -webkit-box-flex: 1;
    flex-grow: 1;
    overflow: auto;
    background-color: rgb(44, 47, 54);
    border-bottom-left-radius: 25px;
    border-bottom-right-radius: 20px;
  }
  .adct-text {
    box-sizing: border-box;
    margin: 0px;
    min-width: 0px;
    font-weight: 400;
    font-size: 16px;
    color: rgb(255, 255, 255);
  }
}

// dialog end

.header {
  display: flex;
  flex-flow: row nowrap;
  width: 100%;
  -webkit-box-pack: justify;
  justify-content: space-between;
}

.h-container {
  display: flex;
  -webkit-box-align: center;
  align-items: center;
  -webkit-box-pack: justify;
  justify-content: space-between;
  flex-direction: column;
  width: 100%;
  top: 0px;
  position: absolute;
  z-index: 2;
}
.hc-container {
  box-sizing: border-box;
  margin: 0px;
  min-width: 0px;
  width: 100%;
  display: flex;
  -webkit-box-align: center;
  // align-items: center;
  padding: 1rem 1rem 0px;
  -webkit-box-pack: justify;
  justify-content: space-between;
  align-items: flex-start;
}

.h-logo {
  display: flex;
  -webkit-box-align: center;
  align-items: center;
}

.h-logo-link {
  display: flex;
  -webkit-box-align: center;
  align-items: center;
  pointer-events: auto;
}
.hll-img {
  transition: transform 0.3s ease 0s;
  img {
    width: 50px;
    outline: none;
  }
  &:hover {
    transform: rotate(-5deg);
  }
}
.hll-text {
  box-sizing: border-box;
  margin: 0px;
  min-width: 0px;
  display: flex;
  padding: 0px;
  -webkit-box-align: center;
  align-items: center;
  width: fit-content;
  white-space: nowrap;
  img {
    margin-left: 4px;
    margin-top: 4px;
  }
}

.h-info {
  display: flex;
  flex-direction: row;
  -webkit-box-align: center;
  align-items: center;
}
.hi-account {
  display: flex;
  -webkit-box-align: center;
  align-items: center;
}

.hic-account {
  display: flex;
  flex-direction: row;
  -webkit-box-align: center;
  align-items: center;
  background-color: rgb(64, 68, 79);
  border-radius: 12px;
  white-space: nowrap;
  width: 100%;
  pointer-events: auto;
}
.hicc-balance {
  box-sizing: border-box;
  margin: 0px;
  min-width: 0px;
  padding-left: 0.75rem;
  padding-right: 0.5rem;
  font-weight: 500;
  flex-shrink: 0;
}
.hicc-connected {
  box-sizing: border-box;
  margin: 0px;
  min-width: 0px;
  appearance: none;
  line-height: inherit;

  text-align: center;
  outline: none;
  text-decoration: none;
  -webkit-box-pack: center;
  justify-content: center;
  position: relative;
  z-index: 1;
  font-size: 16px;
  display: flex;
  flex-flow: row nowrap;
  width: 100%;
  -webkit-box-align: center;
  align-items: center;
  padding: 0.5rem;
  border-radius: 12px;
  cursor: pointer;
  user-select: none;
  background-color: rgb(44, 47, 54);
  border: 1px solid rgb(64, 68, 79);
  color: rgb(255, 255, 255);
  font-weight: 500;
  &:hover {
    background-color: rgb(55, 59, 68);
  }
}
.hiccc-account {
  user-select: none;
  flex: 1 1 auto;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 0px 0.5rem 0px 0.25rem;
  font-size: 1rem;
  width: fit-content;
  font-weight: 500;
  text-align: center;
  color: rgb(255, 255, 255);
}
.network {
  white-space: nowrap;
  width: fit-content;
  margin-left: 10px;
  pointer-events: auto;
  box-sizing: border-box;
  & > div {
    box-sizing: border-box;
    margin: 0px 10px 0px 0px;
    min-width: 0px;
    background-color: rgba(243, 132, 30, 0.05);
    color: rgb(243, 132, 30);
    font-weight: 500;
    width: fit-content;
    border-radius: 12px;
    padding: 8px 12px;
  }
}
.connect-account {
  display: flex;
  -webkit-box-align: center;
  align-items: center;
  .ca-content {
    display: flex;
    flex-direction: row;
    -webkit-box-align: center;
    align-items: center;
    background-color: rgb(33, 36, 41);
    border-radius: 12px;
    white-space: nowrap;
    width: 100%;
    pointer-events: auto;
  }
  .connect-wallet {
    box-sizing: border-box;
    margin: 0px;
    min-width: 0px;
    appearance: none;

    line-height: inherit;
    text-align: center;
    outline: none;
    text-decoration: none;
    -webkit-box-pack: center;
    justify-content: center;
    position: relative;
    z-index: 1;
    font-size: 16px;
    display: flex;
    flex-flow: row nowrap;
    width: 100%;
    -webkit-box-align: center;
    align-items: center;
    padding: 0.5rem;
    border-radius: 12px;
    cursor: pointer;
    user-select: none;
    font-weight: 500;
    background-color: rgba(21, 61, 111, 0.44);
    border: 1px solid rgba(21, 61, 111, 0.44);
    color: rgb(109, 168, 255);
    &:hover {
      border: 1px solid rgba(49, 95, 154, 0.44);
      color: rgb(83, 153, 255);
      background-color: rgba(55, 107, 173, 0.44);
    }
    .cw-text {
      flex: 1 1 auto;
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
      margin: 0px 0.5rem 0px 0.25rem;
      font-size: 1rem;
      width: fit-content;
      font-weight: 500;
    }
  }
}

.hic-operating {
  display: flex;
  -webkit-box-align: center;
  align-items: center;
}
.hico-setting {
  margin-left: 0.5rem;
  display: flex;
  -webkit-box-pack: center;
  justify-content: center;
  -webkit-box-align: center;
  align-items: center;
  position: relative;
  border: none;
  text-align: left;
}
.hicos-button {
  outline: none;
  position: relative;
  width: 100%;
  border: none;
  margin: 0px;
  height: 35px;
  background-color: rgb(64, 68, 79);
  padding: 0.15rem 0.5rem;
  border-radius: 0.5rem;
  user-select: none;
  color: rgb(255, 255, 255);
  font: 400 13.3333px Arial;
  &:hover {
    cursor: pointer;
    outline: none;
    background-color: rgb(86, 90, 105);
  }
  svg {
    height: 20px;
    width: 20px;
    margin-top: 2px;
    stroke: rgb(255, 255, 255);
  }
}

#open-settings-dialog-button {
  outline: none;
}